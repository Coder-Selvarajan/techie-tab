{
    "manifest_version": 2,
    "name": "Techie Tab",
    "description": "Dashboard tab for Techies",
    "version": "1.0.0",
    "icons": {
        "128": "icon_128.png"
    },
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab",
        "storage"
    ],
    "incognito": "split",
    "chrome_url_overrides": {
        "newtab": "index.html"
    }
}